var data = [{"id": "1", "person": 1.0, "event_id": "1", "sequence": 1.0, "choice": 0.0, "background_name": null, "meaning": "ขอโทษนะคะ ที่ตรงนี้ว่างไหมคะ", "dialog": "Excuse me, is this seat available?"}, {"id": "2", "person": 2.0, "event_id": "1", "sequence": 2.0, "choice": 0.0, "background_name": null, "meaning": "ว่างค่ะ", "dialog": "Yes."}, {"id": "3", "person": 1.0, "event_id": "1", "sequence": 3.0, "choice": 0.0, "background_name": null, "meaning": "ขอฉันนั่งด้วยได้มั้ยคะ", "dialog": "Can I sit here?"}, {"id": "4", "person": 2.0, "event_id": "1", "sequence": 4.0, "choice": 0.0, "background_name": null, "meaning": "ได้ค่ะ เชิญค่ะ", "dialog": "Of course! Have a seat."}, {"id": "5", "person": 1.0, "event_id": "1", "sequence": 5.0, "choice": 0.0, "background_name": null, "meaning": "ขอบคุณค่ะ ดีใจที่ได้พบคุณนะคะ", "dialog": "Thank you. It's so nice to meet you."}, {"id": "6", "person": 2.0, "event_id": "1", "sequence": 6.0, "choice": 0.0, "background_name": null, "meaning": "ยินดีที่ได้พบเช่นกันค่ะ", "dialog": "Nice to meet you too."}, {"id": "7", "person": 1.0, "event_id": "1", "sequence": 7.0, "choice": 0.0, "background_name": null, "meaning": "เจ็บไหมอะคะ?", "dialog": "Did it hurt?"}, {"id": "8", "person": 2.0, "event_id": "1", "sequence": 8.0, "choice": 0.0, "background_name": null, "meaning": "คะ? หมายความว่าไงคะ?", "dialog": "Excuse me? What do you mean?"}, {"id": "9", "person": 1.0, "event_id": "1", "sequence": 9.0, "choice": 0.0, "background_name": null, "meaning": "อยากรู้ว่าคุณเจ็บไหมตอนตกมาจากสวรรค์", "dialog": "I want to know if you hurt when you fell from heaven."}, {"id": "10", "person": 2.0, "event_id": "1", "sequence": 10.0, "choice": 0.0, "background_name": null, "meaning": "ตลกตายเลย", "dialog": "That's really funny."}, {"id": "11", "person": 1.0, "event_id": "1", "sequence": 11.0, "choice": 0.0, "background_name": null, "meaning": "คุณอ่านอะไรอยู่หรอคะ", "dialog": "What are you reading?"}, {"id": "12", "person": 2.0, "event_id": "1", "sequence": 12.0, "choice": 0.0, "background_name": null, "meaning": "อ้อ นิตยสารแฟชั่นค่ะ", "dialog": "Oh it's a fashion magazine."}, {"id": "13", "person": 1.0, "event_id": "1", "sequence": 13.0, "choice": 0.0, "background_name": null, "meaning": "นิตยสารนี้แปลกมากเลยแฮะ", "dialog": "This magazine is so strange."}, {"id": "14", "person": 2.0, "event_id": "1", "sequence": 14.0, "choice": 0.0, "background_name": null, "meaning": "ทำไมหรอคะ", "dialog": "Why?"}, {"id": "15", "person": 1.0, "event_id": "1", "sequence": 15.0, "choice": 0.0, "background_name": null, "meaning": "ก็ไม่เห็นว่าจะมีใครสวยกว่าคุณเลย", "dialog": "Because I can't see anybody prettier than you."}, {"id": "16", "person": 2.0, "event_id": "1", "sequence": 16.0, "choice": 0.0, "background_name": null, "meaning": "โอยยย เลี่ยนมากค่า ว่าแต่ คุณเป็นกล้องหรอคะ", "dialog": "That's too sweet! Are you a camera?"}, {"id": "17", "person": 1.0, "event_id": "1", "sequence": 17.0, "choice": 0.0, "background_name": null, "meaning": "??", "dialog": "??"}, {"id": "18", "person": 2.0, "event_id": "1", "sequence": 18.0, "choice": 0.0, "background_name": null, "meaning": "ก็มองคุณทีไรก็ต้องยิ้มทุกทีเลย", "dialog": "Because every time I look at you, I smile."}, {"id": "19", "person": 1.0, "event_id": "1", "sequence": 19.0, "choice": 0.0, "background_name": null, "meaning": "ว้าว! ใช้ได้นะเรา", "dialog": "Wow! What a nice shot!"}, {"id": "20", "person": 2.0, "event_id": "1", "sequence": 20.0, "choice": 0.0, "background_name": null, "meaning": "คุณมีพลาสเตอร์ไหมคะ", "dialog": "Do you have any plaster?"}, {"id": "21", "person": 1.0, "event_id": "1", "sequence": 21.0, "choice": 0.0, "background_name": null, "meaning": "ไม่มีค่ะ อยากให้ฉันไปซื้อให้ไหม", "dialog": "No,I don't. Do you want me to buy it for you?"}, {"id": "22", "person": 2.0, "event_id": "1", "sequence": 22.0, "choice": 0.0, "background_name": null, "meaning": "ดีค่ะ ถ้าไม่ลำบากนะคะ", "dialog": "Yes, please, if you could."}, {"id": "23", "person": 1.0, "event_id": "1", 